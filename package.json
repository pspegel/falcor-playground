{
  "name": "falcor-playground",
  "version": "0.1.0",
  "description": "A playground for trying out Falcor",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && webpack --mode development --config webpack.server.config.ts --config-register @babel/register",
    "build:prod": "rimraf ./dist && webpack --mode production --config webpack.server.config.ts --config-register @babel/register && webpack --mode production --config webpack.prod.config.ts --config-register @babel/register",
    "check-types": "tsc",
    "check-types:watch": "npm run check-types -- --watch",
    "start": "node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pspegel/falcor-playground.git"
  },
  "author": "Peter Spegel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pspegel/falcor-playground/issues"
  },
  "homepage": "https://github.com/pspegel/falcor-playground#readme",
  "babel": {
    "plugins": [
      "@babel/proposal-object-rest-spread",
      "@babel/proposal-class-properties",
      "react-hot-loader/babel"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@types/express": "4.16.1",
    "@types/falcor": "0.1.23",
    "@types/falcor-express": "0.1.18",
    "@types/falcor-router": "0.4.17",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "@types/terser-webpack-plugin": "1.2.0",
    "@types/webpack-dev-middleware": "2.0.2",
    "@types/webpack-hot-middleware": "2.16.5",
    "@types/webpack-node-externals": "1.6.3",
    "babel-loader": "8.0.5",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "react-hot-loader": "4.7.1",
    "rimraf": "2.6.3",
    "terser-webpack-plugin": "1.2.2",
    "typescript": "3.3.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.6.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "express": "4.16.4",
    "falcor": "2.0.5",
    "falcor-express": "0.1.4",
    "falcor-http-datasource": "0.1.3",
    "falcor-router": "0.8.3",
    "react": "16.8.3",
    "react-dom": "16.8.3"
  }
}
